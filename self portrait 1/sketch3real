function setup() {

  createCanvas(500,500);
  background(127,255,212);

}

function draw() {

    ellipse(220,140,80,40);
    fill(0,256,0);
    noStroke();
    
    ellipse(250, 180, 290, 340);
    fill(256,256,256);
    noStroke();
    rect(175,250,150,200);
    fill(256,256,256);

}
